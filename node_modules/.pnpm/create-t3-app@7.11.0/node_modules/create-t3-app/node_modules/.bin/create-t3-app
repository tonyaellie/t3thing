#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/tonya/t3thing/node_modules/.pnpm/create-t3-app@7.11.0/node_modules/create-t3-app/dist/node_modules:/home/tonya/t3thing/node_modules/.pnpm/create-t3-app@7.11.0/node_modules/create-t3-app/node_modules:/home/tonya/t3thing/node_modules/.pnpm/create-t3-app@7.11.0/node_modules:/home/tonya/t3thing/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/tonya/t3thing/node_modules/.pnpm/create-t3-app@7.11.0/node_modules/create-t3-app/dist/node_modules:/home/tonya/t3thing/node_modules/.pnpm/create-t3-app@7.11.0/node_modules/create-t3-app/node_modules:/home/tonya/t3thing/node_modules/.pnpm/create-t3-app@7.11.0/node_modules:/home/tonya/t3thing/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../dist/index.js" "$@"
else
  exec node  "$basedir/../../dist/index.js" "$@"
fi
